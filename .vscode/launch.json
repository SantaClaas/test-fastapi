    "version": "0.2.0",
    "configurations": [

        {
            "name": "wApp (Poetry)",
            "type": "python",
            "request": "launch",
            "module": "poetry",
            "args": [
                "run",
                "uvicorn",
                "webapp.main:app",
                "--reload"
            ],
            "autoReload": {"enable": true},
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "wApp (uvicorn)",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "webapp.main:app",
                "--reload"
            ],
            "autoReload": {"enable": true},
            "jinja": true,
            "justMyCode": true
        }
    ]
}